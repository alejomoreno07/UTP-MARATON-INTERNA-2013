#include <bits/stdc++.h>
using namespace std;
 
int T,k,x,m=-1;
 
void start(){
    cin>>k; m=-1;
    for(int i=0;i<k;i++){
        cin>>x; m=max(m,x);
    }
    cout<<k+1<<" "<<m+1<<endl;
}
 
int main(){
    ios_base::sync_with_stdio(false);
    int i=0;
    cin>>T;
    for(;i<T;){
        if(T==0) return 0;
        cout<<"Case "<<++i<<": ";
        start();
    }
    return 0;
}
